@page
@model TorFutDep.App.Frontend.Pages.Municipios.IndexModel
@{
}
<h1>Listado de Municipio</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var municipio in Model.municipios)
        {
            <tr>
                <td>@municipio.Nombre_Municipio</td>
            </tr>
        }
    </tbody>
</table>