@page
@using TorFutDep.App.Dominio
@model TorFutDep.App.Frontend.Pages.Directores_Tecnicos.CreateModel
@{
}
<h1>Nuevo Director Técnico</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="director_tecnico.Nombre_Director_Tecnico"></label>
        <input asp-for="director_tecnico.Nombre_Director_Tecnico" class="form-control">
        <span asp-validation-for="director_tecnico.Nombre_Director_Tecnico" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="director_tecnico.Documento"></label>
        <input asp-for="director_tecnico.Documento" class="form-control">
        <span asp-validation-for="director_tecnico.Documento" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="director_tecnico.Telefono"></label>
        <input asp-for="director_tecnico.Telefono" class="form-control">
        <span asp-validation-for="director_tecnico.Telefono" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-secondary">Crear Director Técnico</button>
</form>
<a class="btn btn-primary" asp-page="./Index">Volver</a>
